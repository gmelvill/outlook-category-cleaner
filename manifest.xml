<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>f9d6c0c5-1234-4e22-b59e-111111111111</Id>
  <Version>1.0.0.0</Version>
  <ProviderName DefaultValue="Custom Outlook Tools"/>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Category Cleaner"/>
  <Description DefaultValue="Clean up unused categories in your Outlook mailbox."/>
  <IconUrl DefaultValue="https://gmelvill.github.io/outlook-category-cleaner/web/icon.png"/>
  <SupportUrl DefaultValue="https://gmelvill.github.io/outlook-category-cleaner/"/>
  
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  
  <Requirements>
    <Sets DefaultMinVersion="1.8">
      <Set Name="Mailbox"/>
    </Sets>
  </Requirements>

  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://gmelvill.github.io/outlook-category-cleaner/web/taskpane.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadWriteMailbox</Permissions>
</OfficeApp>
